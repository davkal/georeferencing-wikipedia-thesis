	Keywords = {contribution},
	Organization = {IEEE},
	Pages = {581--586},
	Title = {Wikis as digital ecosystems: An analysis based on authorship},
	Year = {2009},
	Bdsk-File-1 = {}}

@inproceedings{jesus2009bipartite,
	Abstract = {This exploratory study investigates the bipartite network of articles linked by common editors in Wikipedia, 'The Free Encyclopedia that Anyone Can Edit'. We use the articles in the categories (to depth three) of Physics and Philosophy and extract and focus on significant editors (at least 7 or 10 edits per each article). We construct a bipartite network, and from it, overlapping cliques of densely connected articles and editors. We cluster these densely connected cliques into larger modules to study examples of larger groups that display how volunteer editors flock around articles driven by interest, real-world controversies, or the result of coordination in WikiProjects. Our results confirm that topics aggregate editors; and show that highly coordinated efforts result in dense clusters.},
	Author = {Jesus, R. and Schwartz, M. and Lehmann, S.},
	Booktitle = {Proceedings of the 5th International Symposium on Wikis and Open Collaboration},
